<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application>
        <activity
            android:name=".tutorial.aboutflow.AboutFlowActivity"
            android:exported="true"
            android:label="@string/title_activity_about_flow"
            android:theme="@style/Theme.AndroidAllCodelabs">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- dpl://main_activity_about_kotlin_flow -->
                <data
                    android:host="main_activity_about_kotlin_flow"
                    android:scheme="dpl" />
            </intent-filter>
        </activity>
    </application>

</manifest>