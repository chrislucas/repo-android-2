<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestAndMockking"
        tools:targetApi="31">
        <activity
            android:name="com.br.feature.codelabs.fromdaggertohil.FromDaggerToHiltActivity"
            android:exported="false"
            android:label="@string/title_activity_from_dagger_to_hilt"
            android:theme="@style/Theme.TestAndMockking" />
        <activity
            android:name="com.br.feature.codelabs.usinghilt.HiltDIActivity"
            android:exported="false"
            android:label="@string/title_activity_hilt_diactivity"
            android:theme="@style/Theme.TestAndMockking" />
        <activity
            android:name="com.br.feature.codelabs.testingcoroutines.TestingCoroutineAndJetpackIntegrationsActivity"
            android:exported="false"
            android:label="@string/title_activity_testing_coroutine_and_jetpack_integrations"
            android:theme="@style/Theme.TestAndMockking" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.TestAndMockking">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>