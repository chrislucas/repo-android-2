<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="modular.xplore.com.br.accessotherapps.fragments.FragmentListAllApps">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:columnOrderPreserved="true"
        android:orientation="vertical"
        android:useDefaultMargins="true">

        <ImageView
            android:id="@+id/icon_icomon_com_vs"
            android:layout_row="0"
            android:layout_column="0"
            android:src="@drawable/logo_app_icomon_com_vc"
            style="@style/DimCelGridImageView"
            />

        <ImageView
            android:id="@+id/icon_app_materiais"
            android:layout_row="0"
            android:layout_column="1"
            android:src="@drawable/logo_app_materiais"
            style="@style/DimCelGridImageView"
            />

        <ImageView
            android:id="@+id/icon_app_abertura_horario"
            android:layout_row="0"
            android:layout_column="2"
            android:src="@drawable/logo_app_abertura_horario"
            style="@style/DimCelGridImageView"
            />

    </GridLayout>





</LinearLayout>
